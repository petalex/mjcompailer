program test_303

const int cinilac = 4, delilac = 3;
enum VrstaPredmeta{ SFERA, KVADAR }
interface IPredmet
{
	int zapremina();
	int Q();
}
class Predmet implements IPredmet
{
	protected int tezina;
	protected VrstaPredmeta vrsta;

	{
		public void postaviTezinu(int tezina) {this.tezina = tezina;}  
		public void ucitajTezinu() { read(tezina); }
		public int zapremina() { return 0;}
		public int Q() {return zapremina() * tezina;}
		public void postaviVrstu(VrstaPredmeta vrsta) {this.vrsta = vrsta;}
		public VrstaPredmeta dohvVrstu() { return vrsta; }
		public int dohvTezinu() { return tezina; }
	}
}

class Sfera extends Predmet
{
	private int r;
	{
		public void postaviTezinu(int tezina) {this.tezina = tezina; vrsta = VrstaPredmeta.SFERA;}
		public void postaviPoluprecnik(int r) {this.r = r; postaviVrstu(VrstaPredmeta.SFERA);}
		public int zapremina()
			int z;
		{
			z = cinilac;
			return z *(r/delilac);
		}
	}
}

class Kvadar extends Predmet
{
	private int a, b, c;
	{
		public void postaviTezinu(int tezina) {this.tezina = tezina; vrsta = VrstaPredmeta.KVADAR;}
		public void postaviStranice(int a, int b, int c) {this.a = a; this.b = b; this.c = c; vrsta = VrstaPredmeta.KVADAR;}
		public int zapremina()
			int z;
		{
			z = a * b * c;
			return z;
		}
	}
}

int zapreminaK, zapreminaS;
int tezinaK, tezinaS;
{

	void ispis( Predmet p)
	{
		if(p.dohvVrstu() == VrstaPredmeta.SFERA) print('S');
		else if(p.dohvVrstu() == VrstaPredmeta.KVADAR) print('K');
		
		print(p.dohvTezinu());

		print(eol);
	}
		
	void main() Predmet predmeti[]; IPredmet interPredmeti[]; int i; Sfera s1, s2, s3; Kvadar k1, k2, k3; int t;
	{
		
		predmeti = new Predmet[6];
		interPredmeti = new IPredmet[6];
		s1 = new Sfera;
		s2 = new Sfera;
		s3 = new Sfera;
		
		k1 = new Kvadar;
		k2 = new Kvadar;
		k3 = new Kvadar;

		s1.postaviPoluprecnik(1);
		s2.postaviPoluprecnik(2);
		s3.postaviPoluprecnik(3);

		k1.postaviStranice(1,2,3);
		k2.postaviStranice(2,3,4);
		k3.postaviStranice(3,4,5);
		
		predmeti[0] = s1;
		predmeti[2] = s2;
		predmeti[4] = s3;
		predmeti[1] = k1;
		predmeti[3] = k2;
		predmeti[5] = k3;

		read(t);
		for(i=0;i<6;i++)
		{
			predmeti[i].postaviTezinu(t);	
