		}

		zapreminaS = 0;
		tezinaS = 0;
		for(i=1;i<=6;i++)
		{
			if(i % 2 == 0) continue; 
			zapreminaS = zapreminaS + interPredmeti[i - 1].zapremina();
			tezinaS = tezinaS + interPredmeti[i - 1].Q();
			ispis(predmeti[i - 1]);
